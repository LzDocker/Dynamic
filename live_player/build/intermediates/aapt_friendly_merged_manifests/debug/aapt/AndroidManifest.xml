<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tencent.liteav.demo.player"
    android:versionCode="100"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="29" />

    <application android:theme="@style/AppTheme" >
        <activity
            android:name="com.tencent.liteav.demo.player.superplayer.SuperPlayerActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" >
            <intent-filter>
                <action android:name="com.tencent.liteav.demo.play.action.float.click" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tencent.liteav.demo.player.VodPlayerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.tencent.liteav.demo.player.common.activity.QRCodeScanActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>